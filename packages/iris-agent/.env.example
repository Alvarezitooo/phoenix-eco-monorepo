# ðŸ¤– IRIS AGENT - Configuration Environnement

# === API KEYS ===
GEMINI_API_KEY=your_gemini_api_key_here

# === PHOENIX LETTERS INTEGRATION ===
PHOENIX_DB_URL=your_supabase_url_here
PHOENIX_DB_KEY=your_supabase_anon_key_here
PHOENIX_JWT_SECRET=your_jwt_secret_key_here

# === IRIS CONFIGURATION ===
IRIS_ENV=development
IRIS_LOG_LEVEL=INFO
IRIS_CACHE_DURATION=300

# === SECURITY SETTINGS ===
IRIS_RATE_LIMIT_REQUESTS=15
IRIS_RATE_LIMIT_WINDOW=60
IRIS_MAX_MESSAGE_LENGTH=500

# === MONITORING ===
IRIS_ENABLE_METRICS=true
IRIS_METRICS_PORT=8004

# === DEVELOPMENT ===
IRIS_DEBUG_MODE=false
IRIS_MOCK_PHOENIX_EVENTS=true