{
  "name": "phoenix-rise-monorepo",
  "private": true,
  "version": "0.1.0",
  "description": "Phoenix Rise & Dojo Mental - Mental traction ecosystem",
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:web\"",
    "dev:web": "cd apps/web && npm run dev",
    "dev:api": "cd apps/api && uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "build": "npm run build:web && npm run build:api",
    "build:web": "cd apps/web && npm run build",
    "build:api": "cd apps/api && pip install -r requirements.txt",
    "test": "npm run test:web && npm run test:api",
    "test:web": "cd apps/web && npm run test",
    "test:api": "cd apps/api && pytest",
    "lint": "npm run lint:web && npm run lint:api",
    "lint:web": "cd apps/web && npm run lint",
    "lint:api": "cd apps/api && mypy . && flake8",
    "seed": "cd apps/api && python scripts/seed_data.py",
    "migrate": "cd apps/api && alembic upgrade head"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "keywords": ["phoenix", "rise", "dojo", "mental", "kaizen", "zazen"],
  "author": "Phoenix Tech Team",
  "license": "PROPRIETARY"
}