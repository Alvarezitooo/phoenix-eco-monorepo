{
  "name": "phoenix-aube",
  "version": 2,
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "rewrites": [
    {
      "source": "/api/v1/:path*",
      "destination": "https://phoenix-aube-production.up.railway.app/api/v1/:path*"
    },
    {
      "source": "/health",
      "destination": "https://phoenix-aube-production.up.railway.app/health"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}